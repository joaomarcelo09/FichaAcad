generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model dinheiro {
  id                                                     Int               @id @default(autoincrement())
  id_despesa                                             Int?
  id_receita                                             Int?
  dinheiro_despesa_dinheiro_id_despesaTodinheiro_despesa dinheiro_despesa? @relation("dinheiro_id_despesaTodinheiro_despesa", fields: [id_despesa], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "FK_dinheiro_dinheiro_despesa")
  dinheiro_despesa_dinheiro_id_receitaTodinheiro_despesa dinheiro_receita? @relation("dinheiro_id_receitaTodinheiro_despesa", fields: [id_receita], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "FK_dinheiro_dinheiro_despesa_2")

  @@index([id_despesa], map: "FK_dinheiro_dinheiro_despesa")
  @@index([id_receita], map: "FK_dinheiro_dinheiro_receita")
}

model dinheiro_despesa {
  id                                             Int        @id
  valor                                          Int
  dinheiro_dinheiro_id_despesaTodinheiro_despesa dinheiro[] @relation("dinheiro_id_despesaTodinheiro_despesa")
}

model dinheiro_receita {
  id                                             Int        @id
  valor                                          Int
  dinheiro_dinheiro_id_receitaTodinheiro_despesa dinheiro[] @relation("dinheiro_id_receitaTodinheiro_despesa")
}
